<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:id="@+id/backlayout"    android:layout_width="fill_parent"    android:orientation="vertical"    android:layout_height="fill_parent">    <RelativeLayout        android:id="@+id/relativeLayout_title"        android:layout_width="fill_parent"        android:layout_height="50dp"        android:background="@drawable/titlebar_lightgray_bg">        <ImageView            android:id="@+id/button_back"            android:layout_width="50dp"            android:layout_height="50dp"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:src="@drawable/back_button" />        <TextView            android:id="@+id/title"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerInParent="true"            android:layout_gravity="center_vertical"            android:gravity="center"            android:shadowColor="#ff000000"            android:shadowDx="3.0"            android:shadowDy="3.0"            android:shadowRadius="3.0"            android:text="智慧化学习"            android:textAppearance="?android:attr/textAppearanceLarge"            android:textColor="@color/whitesmoke" />    </RelativeLayout>    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_margin="8dp"        android:text="@string/my_intel_test"        android:textColor="#000000"        android:textSize="16sp" />    <LinearLayout        android:layout_width="fill_parent"        android:layout_height="wrap_content"        android:layout_marginLeft="10dp"        android:layout_marginRight="10dp"        android:background="@drawable/me_item_bg"        android:orientation="vertical" >        <RelativeLayout            android:id="@+id/intel_ability_test"            android:layout_width="fill_parent"            android:layout_height="50dp" >            <ImageView                android:id="@+id/im0"                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_marginLeft="10dp"                android:src="@drawable/my_intel_test_logo" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_margin="10dp"                android:layout_toRightOf="@id/im0"                android:drawablePadding="5dp"                android:gravity="center"                android:text="@string/my_intel_test"                android:textColor="#444444"                android:textSize="16sp" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="10dp"                android:src="@drawable/icon_right" />        </RelativeLayout>    </LinearLayout>    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_margin="8dp"        android:text="@string/my_intel_learning"        android:textColor="#000000"        android:textSize="16sp" />    <LinearLayout        android:layout_width="fill_parent"        android:layout_height="wrap_content"        android:layout_marginLeft="10dp"        android:layout_marginRight="10dp"        android:background="@drawable/me_item_bg"        android:orientation="vertical" >        <RelativeLayout            android:id="@+id/intel_userinfo"            android:layout_width="fill_parent"            android:layout_height="50dp" >            <ImageView                android:id="@+id/iv_mine_que_myque2"                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_marginLeft="10dp"                android:src="@drawable/mine_intel_myinfo" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_margin="10dp"                android:layout_toRightOf="@id/iv_mine_que_myque2"                android:drawablePadding="5dp"                android:gravity="center"                android:text="@string/userinfo_complete"                android:textColor="#444444"                android:textSize="16sp" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="10dp"                android:src="@drawable/icon_right" />        </RelativeLayout>        <ImageView            android:layout_width="fill_parent"            android:layout_height="0.5dp"            android:background="#dcdcdc" />        <RelativeLayout            android:id="@+id/intel_goal"            android:layout_width="fill_parent"            android:layout_height="50dp" >            <ImageView                android:id="@+id/iv_mine_que_myque3"                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_marginLeft="10dp"                android:src="@drawable/mine_intel_goal" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_margin="10dp"                android:layout_toRightOf="@id/iv_mine_que_myque3"                android:drawablePadding="5dp"                android:gravity="center"                android:text="@string/intel_learning_goal"                android:textColor="#444444"                android:textSize="16sp" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="10dp"                android:src="@drawable/icon_right" />        </RelativeLayout>        <ImageView            android:layout_width="fill_parent"            android:layout_height="0.5dp"            android:background="#dcdcdc" />        <RelativeLayout            android:id="@+id/intel_result"            android:layout_width="fill_parent"            android:layout_height="50dp" >            <ImageView                android:id="@+id/iv_mine_que_myque4"                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_marginLeft="10dp"                android:src="@drawable/mine_intel_result" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_margin="10dp"                android:layout_toRightOf="@id/iv_mine_que_myque4"                android:drawablePadding="5dp"                android:gravity="center"                android:text="@string/intel_learning_result"                android:textColor="#444444"                android:textSize="16sp" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="10dp"                android:src="@drawable/icon_right" />        </RelativeLayout>        <ImageView            android:layout_width="fill_parent"            android:layout_height="0.5dp"            android:background="#dcdcdc" />        <RelativeLayout            android:id="@+id/intel_test_result"            android:layout_width="fill_parent"            android:layout_height="50dp" >            <ImageView                android:id="@+id/iv_mine_que_myque5"                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_marginLeft="10dp"                android:src="@drawable/mine_intel_test" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_margin="10dp"                android:layout_toRightOf="@id/iv_mine_que_myque5"                android:drawablePadding="5dp"                android:gravity="center"                android:text="@string/intel_test_result"                android:textColor="#444444"                android:textSize="16sp" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="10dp"                android:src="@drawable/icon_right" />        </RelativeLayout>        <ImageView            android:layout_width="fill_parent"            android:layout_height="0.5dp"            android:background="#dcdcdc" />        <RelativeLayout            android:id="@+id/intel_word_result"            android:layout_width="fill_parent"            android:layout_height="50dp" >            <ImageView                android:id="@+id/iv_mine_que_myque6"                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_marginLeft="10dp"                android:src="@drawable/mine_intel_word" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_margin="10dp"                android:layout_toRightOf="@id/iv_mine_que_myque6"                android:drawablePadding="5dp"                android:gravity="center"                android:text="@string/intel_word_result"                android:textColor="#444444"                android:textSize="16sp" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="10dp"                android:src="@drawable/icon_right" />        </RelativeLayout>    </LinearLayout></LinearLayout>